      "dest": "/api/schedulings"
    },
    {
      "src": "/api/schedulings/:id",
      "method": "DELETE",
      "dest": "/api/schedulings/:id"
    },
    {
      "src": "/api/datehour",
      "method": "GET",
      "dest": "/api/datehour"
    },
    {
      "src": "/entrar",
